<div class="content">
  <div class="col col-2 bg-gray p2">
    <ul>
      <% @tags.each do |tag| %>
        <li><%= tag.name %></li>
      <% end %>
    </ul>
    <%= link_to 'New Image', new_image_path %>
  </div>
  <div class="col col-10">
    <div class="images col-12">
      <% @images.each do |image| %>
        <%= image_tag image.file.url, class: 'border-box'%>
      <% end %>
    </div>
  </div>
</div>
